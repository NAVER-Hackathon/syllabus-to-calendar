{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	],
	"env": {
		"PORT": "3001",
		"DB_HOST": "@db_host",
		"DB_USER": "@db_user",
		"DB_PASSWORD": "@db_password",
		"DB_NAME": "@db_name",
		"SECRET_KEY_OCR": "@secret_key_ocr",
		"CLOVA_OCR_URL": "@clova_ocr_url",
		"CLOVA_STUDIO_API_KEY": "@clova_studio_api_key",
		"CLOVA_STUDIO_URL": "@clova_studio_url"
	}
}
